    item biscuit = new item();

    biscuit.price = 75;
    biscuit.weight = 30.50f;
    biscuit.quantity = 69;
    biscuit.description = "This biscuit can make your day better";
    biscuit.canbetraded = false;
    
    biscuit.sayMyDescription();
    biscuit.sayMyPrice();
    biscuit.sayMyWeight(); 
    biscuit.sayMyQuantity();
    biscuit.sayMyCanbetraded();

